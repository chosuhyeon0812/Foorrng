<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_gravity="start"
    android:orientation="vertical"
    tools:context=".foodtruck.info.FoodtruckInfoTabFragment">

    <TextView
        style="@style/Foorrng.TEXT.EB.16"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="14dp"
        android:text="@string/all_foodtruck_name"
        android:textColor="@color/foorrng_orange_dark" />

    <TextView
        android:id="@+id/tv_truck_name"
        style="@style/Foorrng.TEXT.SB.16"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        tools:text="테스트테스트테스트123456789" />

    <TextView
        style="@style/Foorrng.TEXT.EB.16"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="14dp"
        android:text="@string/all_busi_number"
        android:textColor="@color/foorrng_orange_dark" />

    <TextView
        android:id="@+id/tv_business_number"
        style="@style/Foorrng.TEXT.SB.16"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        tools:text="테스트테스트테스트123456789" />

    <TextView
        style="@style/Foorrng.TEXT.EB.16"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
