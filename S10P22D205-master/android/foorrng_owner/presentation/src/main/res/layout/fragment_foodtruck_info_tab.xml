        android:layout_marginTop="5dp"
        tools:text="테스트테스트테스트123456789" />

    <TextView
        style="@style/Foorrng.TEXT.EB.16"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="14dp"
        android:text="@string/all_account_number"
        android:textColor="@color/foorrng_orange_dark" />

    <TextView
        android:id="@+id/tv_account_number"
        style="@style/Foorrng.TEXT.SB.16"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        tools:text="테스트테스트테스트123456789" />

    <TextView
        style="@style/Foorrng.TEXT.EB.16"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="14dp"
        android:text="@string/all_call_number"
        android:textColor="@color/foorrng_orange_dark" />

    <TextView
        android:id="@+id/tv_call_number"
        style="@style/Foorrng.TEXT.SB.16"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        tools:text="테스트테스트테스트123456789" />

    <TextView
        style="@style/Foorrng.TEXT.EB.16"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="14dp"
        android:text="@string/all_food_category"
        android:textColor="@color/foorrng_orange_dark" />

    <TextView
        android:id="@+id/tv_food_category"
        style="@style/Foorrng.TEXT.SB.16"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        tools:text="테스트테스트테스트123456789" />

    <TextView
